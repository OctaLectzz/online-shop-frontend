{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["prettier-plugin-tailwindcss"],
  "printWidth": 500,
  "max-len": [
    2,
    {
      "code": 550,
      "tabWidth": 2,
      "ignoreUrls": true,
      "ignoreStrings": true
    }
  ],
  "semi": false,
  "singleQuote": true,
  "htmlWhitespaceSensitivity": "ignore",
  "trailingComma": "none",
  "bracketSpacing": true
}
